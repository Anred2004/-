// Задание 5.1
#include <iostream>
using namespace std;

int main()
{
	char str[100];
	cin >> str;
	char sogl[100] = "bcdfghjklmnpqrstvwxz";
	for (int i = 0; i < strlen(str); i++)
	{
		for (int j = 0; j < strlen(sogl); j++)
		{
			if (str[i] == sogl[j])
			{
				str[i] = str[i] - 32;
				break;
			}
		}

	}
	cout << str;
	system("pause");
}


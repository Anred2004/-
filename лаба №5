	string str; 
	string s[1005];
	int N, size = 0;
	char first, second;
	cin >> N >> first >> second;
	ifstream fin("input.txt"); 
	ofstream fout("output.txt");
	while (fin >> str) 
	{
		firstBool = false;
		secondBool = false;
		for (int i = 0; i < str.length(); i++)
		{
			if (str[i] == first)
			{
				firstBool = true;
			}
			if (str[i] == second)
			{
				secondBool = true;
			}

			

		}
		

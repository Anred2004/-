def combinations():
    word = "КОМБИНАТОРИКА"
    word = list(word) #разделяем слова на буквы
    combination = set() #множество 7-буквенных слов(неповторяющихся)
    for i in range(len(word)):
        for j in range(len(word)):
            for k in range(len(word)):
                for f in range(len(word)):
                    for h in range(len(word)):
                        for l in range(len(word)):
                            for m in range(len(word)):
                                if i==j or i==k or i==f or i==h or i==l or i==m or j==k or j==f or j==h or j==l or j==m or k==f or k==h or k==l or k==m or f==h or f==l or f==m or h==l or h==m or l==m:
                                    continue # исключаем излишние повторения
                                combination.add(word[i] + word[j] + word[k]+ word[f] + word[h]+ word[l] + word[m]) # состовляем слова из 7-ми букв перебором
    print('слов без повторений' , len(combination))
combinations()

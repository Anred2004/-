// Задание 1
#include <iostream>
using namespace std;
int main()
{
	int n;
	bool flag = false;
	int arr[10005];
	cin >> n;
	for (int i = 0; i < n; i++)
	{
		cin >> arr[i];
	}
	for (int i = 0; i < n - 1; i++)
	{
		for (int j = i + 1; j < n; j++)
		{
			if (arr[i] == arr[j])
			{
				flag = true;
				break;
			}

		}
		if (flag == true)
		{
			break;
		}
	}

	if (flag == true)
	{
		for (int i = 0; i < n - 1; i++)
		{
			for (int j = i + 1; j < n; j++)
			{
				if (arr[i] % 10 < arr[j] % 10)
					swap(arr[i], arr[j]);
			}

		}
	}
	for (int i = 0; i < n; i++)
	{
		cout << arr[i] << " ";
	}
	system("pause");
}
// Задание 2
#include <iostream>
using namespace std;
int main()
{
	int n;

	int digits[1005];
	bool flag = false;
	int arr[1005];
	int mult[10000];
	cin >> n;
	for (int i = 0; i < n; i++)
	{
		cin >> arr[i];
		int tmp = arr[i];
		while (tmp > 9)
			tmp /= 10;
		digits[i] = tmp;
		tmp = arr[i];
		int p = 1;
		while (tmp > 0) {
			p *= tmp % 10;
			tmp /= 10;
		}
		mult[i] = p;
		
		
	}
	for (int i = 0; i < n - 1; i++)
	{
		for (int j = i + 1; j < n; j++)
		{
			if (digits[i] > digits[j])
			{
				swap(digits[i], digits[j]);
				swap(arr[i], arr[j]);
				swap(mult[i], mult[j]);
			}
		}
	}
		for (int i = 0; i < n - 1; i++)
		{
			for (int j = i + 1; j < n; j++)
			{
				if ((digits[i] == digits[j]) && (mult[i] > mult[j]))
				{
					swap(mult[i], mult[j]);
					swap(digits[i], digits[j]);
					swap(arr[i], arr[j]);
					
				}
			}
		}
		for (int i = 0; i < n - 1; i++)
		{
			for (int j = i + 1; j < n; j++)
			{
				if ((digits[i] == digits[j]) && (mult[i] == mult[j]) && (arr[i] > arr[j]))
				{
					swap(mult[i], mult[j]);
					swap(digits[i], digits[j]);
					swap(arr[i], arr[j]);

				}
			}
		}
	
	for (int i = 0; i < n;i++) {
		cout << arr[i] << " ";
	}
	cout << endl;
	for (int i = 0; i < n;i++) {
		cout << digits[i] << " ";
	}
	cout << endl;
	
	for (int i = 0; i < n;i++) {
		cout << mult[i] << " ";
	}
	cout << endl;

	system("pause");
}
